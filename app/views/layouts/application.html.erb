<!DOCTYPE html>
<html>
<head>
  <title>PWWDashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<!-- Temp navbar for dev -->
<% if user_signed_in? %>
    <%= render 'layouts/nav' %>
<% end %>

<!--Temp placement of notifications-->
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
 
<div class="container">      
  <%= yield %>
</div>

</body>
</html>
